{% extends 'base.html.twig' %}

{% block title %}ConnectedDevice{% endblock %}

{% block content %}
    <h1>ConnectedDevice</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ connected_device.id }}</td>
            </tr>
            <tr>
                <th>DeviceId</th>
                <td>{{ connected_device.deviceId }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ connected_device.name }}</td>
            </tr>
            <tr>
                <th>Location</th>
                <td>{{ connected_device.location }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_connected_device_index') }}">back to list</a>

    <a href="{{ path('app_connected_device_edit', {'id': connected_device.id}) }}">edit</a>

    {{ include('connected_device/_delete_form.html.twig') }}
{% endblock %}
